{
  "name": "@wluwd/common",
  "version": "0.0.1",
  "type": "module",
  "typesVersions": {
    "*": {
      "assertion-error": [
        "dist/assertion-error.d.ts"
      ],
      "log": [
        "dist/log.d.ts"
      ],
      "message-symbols": [
        "dist/message-symbols.d.ts"
      ],
      "result": [
        "dist/result.d.ts"
      ]
    }
  },
  "exports": {
    "./assertion-error": {
      "import": "./dist/assertion-error.js",
      "types": "./dist/assertion-error.d.ts"
    },
    "./log": {
      "import": "./dist/log.js",
      "types": "./dist/log.d.ts"
    },
    "./message-symbols": {
      "import": "./dist/message-symbols.js",
      "types": "./dist/message-symbols.d.ts"
    },
    "./result": {
      "import": "./dist/result.js",
      "types": "./dist/result.d.ts"
    }
  },
  "scripts": {
    "clean": "del dist",
    "compile:dts": "tsc",
    "compile:ts": "esbuild --outdir=dist source/*.ts source/**/*.ts",
    "lint": "xo --cwd ../.. packages/common/source",
    "typecheck": "tsc --emitDeclarationOnly false --noEmit"
  },
  "dependencies": {
    "kleur": "^4.1.4"
  }
}
